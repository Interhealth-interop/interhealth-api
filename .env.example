APP_PORT=3000
NODE_ENV=local

MONGO_URL=mongodb+srv://username:password@cluster.mongodb.net/database?appName=AppName
JWT_SECRET=your-secret-key-here
RUST_LOG=debug

MAX_CONCURRENT_JOBS=5

# Simulate failures for testing metrics (optional)
# If not set, no failures will be simulated
# Value should be between 0.0 (no failures) and 1.0 (100% failure)
# Example: 0.20 = 20% failure rate
# SIMULATED_FAILURE_RATE=0.20

# RabbitMQ Configuration (optional)
# RMQ_QUEUE=datamea
# RMQ_HOSTNAME=hostname
# RMQ_USERNAME=user
# RMQ_PASSWORD=password
# RMQ_PREFETCH=100
